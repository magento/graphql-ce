<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../../../../../vendor/magento/mtf/etc/variations.xsd">
    <testCase name="Magento\Backend\Test\TestCase\NavigateMenuTest">
        <variation name="NavigateMenuTest20">
            <data name="tag" xsi:type="string">mftf_migrated:yes</data>
            <data name="menuItem" xsi:type="string">Stores > Currency Rates</data>
            <data name="pageTitle" xsi:type="string">Currency Rates</data>
            <constraint name="Magento\Backend\Test\Constraint\AssertBackendPageIsAvailable"/>
        </variation>
        <variation name="NavigateMenuTest21">
            <data name="tag" xsi:type="string">mftf_migrated:yes</data>
            <data name="menuItem" xsi:type="string">Stores > Currency Symbols</data>
            <data name="pageTitle" xsi:type="string">Currency Symbols</data>
            <constraint name="Magento\Backend\Test\Constraint\AssertBackendPageIsAvailable"/>
        </variation>
    </testCase>
</config>
